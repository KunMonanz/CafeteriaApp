@{
    ViewData["Title"] = "Home";
}
@section Styles {
    <link rel="stylesheet" href="style.css" />
}

<body>
  <header>
    <nav class="navbar navbar-expand-lg navbar-light bg-light shadow-sm">
      <div class="container">
        <a class="navbar-brand fw-bold logo" href="#"><img src="images/jubilee logo.fw.png" alt=""></a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarNav"
          aria-controls="navbarNav"
          aria-expanded="false"
          aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse justify-content-end" id="navbarNav">
          <ul class="navbar-nav fw-semibold">
            <li class="nav-item">
              <a class="nav-link active" href="index.html">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="menu.html">Menu</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="order.html">Order</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#contact">Contact</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
       
    <div id="carouselExampleSlidesOnly" class="carousel slide jubhero" data-bs-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="images/Untitled-1.fw.png" class="d-block w-100" alt="...">
        </div>

        <div class="carousel-item">
          <img src="images/Untitled-4.fw.png" class="d-block w-100" alt="...">
        </div>

        <div class="carousel-item">
          <img src="images/Untitled-3.fw.png" class="d-block w-100" alt="...">
        </div>
                
      </div>
    </div>
  </header>

  <section class="hero">
    <div class="container text-center">
      <h1 class="display-3 fw-bold">Welcome to <span class="orange">JU</span><span class="yellow">BIL</span><span class="orange">EE</span></h1>
      <p class="lead ">
        Your home for delicious, authentic Nigerian meals.
      </p>
      <p class="lead ">
        Whether you're craving smoky Jollof Rice, soft Fufu with rich Egusi soup, spicy Amala, or any of your favorite local dishes, we've got you covered. Our meals are prepared fresh every day with love, flavor, and tradition in every bite.
      </p>
      <p class="lead ">Sit back, relax, and let the taste of Naija come to you.Good food. Great vibes. Real Nigerian flavor.</p>
      <a href="menu.html" asp-controller="Menu"  asp-action="menulist" class="btn btn-warning btn-lg fw-semibold">Explore Menu</a>
    </div>
  </section>

  <section class="menusec" id="menu" class="py-5">
    <div class="container">
      <h2 class="text-center mb-4 fw-bold">Our <span class="orange">Specialties</span> </h2>
      <div class="row g-4">
        <div class="col-md-4 menuhome">
          <div class="card menu-card ">
            <img src="images/fried rice.jpeg" class="card-img-top" alt="Cappuccino" />
            <div class="card-body">
              <h5 class="card-title">Fried Rice</h5>
              <p class="card-text">Tantaliizing Fried rice with fresh, green and tasty vegetables.</p>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="card menu-card ">
            <img src="images/jellof spag.jpeg" class="card-img-top" alt="Blueberry Muffin" />
            <div class="card-body">
              <h5 class="card-title">Jellof Spag</h5>
              <p class="card-text">Tantaliizing Jellof Spag with fresh, green and tasty vegetables.</p>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="card menu-card ">
            <img src="images/Nigerian-Jollof-Rice.jpg" class="card-img-top" alt="Avocado Toast" />
            <div class="card-body">
              <h5 class="card-title">Jollof Rice</h5>
              <p class="card-text">Tantaliizing Jollof rice with fresh, green and tasty vegetables.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>


  <div class="slide_overall">

    <h1>Some Reviews</h1>
    <div id="carouselExampleDark" class="slide_container carousel carousel-dark slide">
      <div class="carousel-indicators mt-3">
        <button type="button" data-bs-target="#carouselExampleDark" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#carouselExampleDark" data-bs-slide-to="1" aria-label="Slide 2"></button>
        <button type="button" data-bs-target="#carouselExampleDark" data-bs-slide-to="2" aria-label="Slide 3"></button>
      </div>
      <div class="slide_content carousel-inner">
        <div class="carousel-item flex active" data-bs-interval="10000">
          <div class="make_flex">
            <div class="slide_wrapper">
              <div class="ncard">
                <div class="image_content">
                  <span class="overlay"></span>
                  <div class="card_image">
                    <img src="images/review1.fw.png" alt="" class="card_img">
                  </div>
                </div>
        
                <div class="card_content">
                  <h2 class="name">Emily</h2>
                    <p class="description">"Enjoyed the service and more importantly the food. Had the Chicago-style chicken and waffles. Would’ve loved a larger portion though..</p>   
                </div>
              </div>
            </div>
            <div class="slide_wrapper display_none_card">
              <div class="ncard">
                <div class="image_content">
                  <span class="overlay"></span>
                  <div class="card_image">
                    <img src="images/review2.fw.png" alt="" class="card_img">
                  </div>
                </div>
        
                <div class="card_content">
                  <h2 class="name">Rachel</h2>
                  <p class="description">"The staff are very courteous! Food is excellent! My friends and I were extremely happy! Great space! Nice ambience, and beautiful people!"</p>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="carousel-item flex" data-bs-interval="10000">
          <div class="make_flex">
                        <div class="slide_wrapper">
                            <div class="ncard">
                                <div class="image_content">
                                    <span class="overlay"></span>
                                    <div class="card_image">
                                        <img src="images/review3.fw.png" alt="" class="card_img">
                                    </div>
                                </div>
        
                                <div class="card_content">
                                    <h2 class="name">Jemimah</h2>
                                    <p class="description">"Been there over half a dozen times, and I've never had a bad meal. The only complaint is the small space, which can crowd pretty early, especially on weekends, but it is what it is. Food is always fantastic!"</p>
                                    
                                </div>
                            </div>
                        </div>
                        <div class="slide_wrapper display_none_card">
                            <div class="ncard">
                                <div class="image_content">
                                    <span class="overlay"></span>
                                    <div class="card_image">
                                        <img src="images/review4.fw.png" alt="" class="card_img">
                                    </div>
                                </div>
        
                                <div class="card_content">
                                    <h2 class="name">Jimisola</h2>
                                    <p class="description">"I recommend the mac and cheese but not the price. Prawns will always be served as 2. Blueberry pancakes are cool. Pork sausage and bacon available but you've got to ask them to make it crispy to avoid undercooked pork."</p>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="carousel-item flex" data-bs-interval="10000">
                    <div class="make_flex">
                        <div class="slide_wrapper">
                            <div class="ncard">
                                <div class="image_content">
                                    <span class="overlay"></span>
                                    <div class="card_image">
                                        <img src="images/review5.fw.png" alt="" class="card_img">
                                    </div>
                                </div>
        
                                <div class="card_content">
                                    <h2 class="name">Pelumi</h2>
                                    <p class="description">"We had a great family time at Cafeteria... service was really good, waiters are on standby ready to help, food was great! Lots of yummy goodness to pick from, and sizable portions too!"</p>
                                    
                                </div>
                            </div>
                        </div>
                        <div class="slide_wrapper display_none_card">
                            <div class="ncard">
                                <div class="image_content">
                                    <span class="overlay"></span>
                                    <div class="card_image">
                                        <img src="images/review6.fw.png" alt="" class="card_img">
                                    </div>
                                </div>
        
                                <div class="card_content">
                                    <h2 class="name">Tomiwa</h2>
                                    <p class="description">"I loved it here and the service was splendid. Would definitely recommend this restaurant for a nice day out with friends!"</p>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleDark" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
              </button>
              <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleDark" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
              </button>
        </div>
    </div>


  <!-- Contact Section -->
  <section id="contact" class="py-4 contacthome">
    <div class="container text-center">
      <h3 class="mb-3 fw-bold">Get in Touch</h3>
      <p>Email: info@JubileeCaf.com | Phone: +234 811 5797 867</p>
      <p>106 Adedokun road, Ayobo Ipaja, Lagos</p>
    </div>
  </section>

  
  <footer id="footer" class="footer dark-background">

    <div class="container">
      <div class="row gy-3">
        <div class="col-lg-3 col-md-6 d-flex">
          <i class="bi bi-geo-alt icon"></i>
          <div class="address">
            <h4>Address</h4>
            <p>106 Adedokun road</p>
            <p>Ayobo Ipaja, Lagos</p>
            <p></p>
          </div>

        </div>

        <div class="col-lg-3 col-md-6 d-flex">
          <i class="bi bi-telephone icon"></i>
          <div>
            <h4>Contact</h4>
            <p>
              <strong>Phone:</strong> <span>+234 0811 579 7867</span><br>
              <strong>Email:</strong> <span>info@JubileeCaf.com</span><br>
            </p>
          </div>
        </div>

        <div class="col-lg-3 col-md-6 d-flex">
          <i class="bi bi-clock icon"></i>
          <div>
            <h4>Opening Hours</h4>
            <p>
              <strong>Mon-Sat:</strong> <span>11AM - 23PM</span><br>
              <strong>Sunday</strong>: <span>Closed</span>
            </p>
          </div>
        </div>

        <div class="col-lg-3 col-md-6">
          <h4>Follow Us</h4>
          <div class="social-links d-flex">
            <a href="#" class="twitter"><i class="bi bi-twitter-x"></i></a>
            <a href="#" class="facebook"><i class="bi bi-facebook"></i></a>
            <a href="#" class="instagram"><i class="bi bi-instagram"></i></a>
            <a href="#" class="linkedin"><i class="bi bi-linkedin"></i></a>
          </div>
        </div>

      </div>
    </div>

    <div class="container copyright text-center mt-4">
      <p>© <span>Copyright</span> <strong class="px-1 sitename">Jubilee</strong> <span>All Rights Reserved</span></p>
    </div>

  </footer>

    

    <script src="bootstrap-5.3.6-dist/js/bootstrap.bundle.js"></script>
</body>
